<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gas.Securitycheck" >

    <application
        android:name=".ServiceApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.SignIn"
            android:label="安检系统" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Task"
            android:label="任务" >
        </activity>
        <activity
            android:name=".activity.TaskList"
            android:label="任务列表" >
        </activity>
        <activity
            android:name=".activity.enterRegister"
            android:label="入户签到" >
        </activity>
        <activity
            android:name=".activity.SecurityItem"
            android:label="安检项" >
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:label="主页面" >
        </activity>
        <activity
            android:name=".activity.DangerIevel"
            android:label="未使用"
            >
        </activity>
    </application>

</manifest>
